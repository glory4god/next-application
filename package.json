{
  "name": "turborepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@yarnpkg/pnpify": "4.0.0-rc.26",
    "eslint": "8.13.0",
    "prettier": "latest",
    "turbo": "^1.6.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "npm": "please-use-yarn",
    "node": ">=16.0.0",
    "yarn": "^3"
  },
  "engineStrict": true,
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "react": "latest",
    "react-dom": "latest"
  }
}
